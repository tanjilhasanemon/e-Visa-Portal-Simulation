<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="566.0" prefWidth="700.0" style="-fx-background-color: #36454F;"
            xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.project.visa_management_portal.tanjil.applicant.controller.FeedbackController">
    <Label layoutX="240.0" layoutY="20.0" text="Help and Feedback" textFill="#00a1ec" underline="true">
        <font>
            <Font name="System Bold" size="26.0"/>
        </font>
    </Label>

    <Label layoutX="20.0" layoutY="177.0" text="Subject:" textFill="white"/>
    <TextField fx:id="subjectTextField" layoutX="120.0" layoutY="173.0" prefWidth="560.0"/>

    <Label layoutX="20.0" layoutY="217.0" text="Message:" textFill="white"/>
    <TextArea fx:id="messageTextArea" layoutX="20.0" layoutY="247.0" prefHeight="220.0" prefWidth="660.0"/>
    <Button layoutX="160.0" layoutY="487.0" onAction="#submitOnAction" prefWidth="120.0"
            style="-fx-background-color: #00a1ec;" text="Submit"/>
    <Button layoutX="300.0" layoutY="487.0" onAction="#backToDashboardOnAction" prefWidth="160.0"
            style="-fx-background-color: #00a1ec;" text="Back to Dashboard"/>
    <Label alignment="CENTER" layoutY="546.0" prefHeight="20.0" prefWidth="700.0" style="-fx-background-color: white;"
           text="eVisa Portal | Feedback"/>
    <TextField fx:id="applicantIDTextField" layoutX="125.0" layoutY="110.0"/>
    <Label layoutX="20.0" layoutY="114.0" text="Applicant ID:" textFill="WHITE"/>
</AnchorPane>
