    <children>
        <Label fx:id="lblTitle" text="Processing Time Settings" AnchorPane.topAnchor="16.0" AnchorPane.leftAnchor="16.0"/>

        <TableView fx:id="tblProcessingTimes" AnchorPane.topAnchor="56.0" AnchorPane.leftAnchor="16.0" AnchorPane.rightAnchor="16.0" AnchorPane.bottomAnchor="80.0">
            <columns>
                <TableColumn fx:id="colVisaType" text="VisaType"/>
                <TableColumn fx:id="colProcessingDays" text="ProcessingDays"/>
            </columns>
        </TableView>

        <TextField fx:id="txtEditDays" promptText="Enter days" AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="16.0" prefWidth="120.0"/>
        <Button fx:id="btnSave" text="Save" AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="150.0"/>
